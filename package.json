{
  "name": "secrets-sync-cli",
  "version": "1.0.6-20251126.9",
  "description": "CLI tool for syncing environment secrets across environments with drift detection and GitHub Secrets integration",
  "type": "module",
  "bin": {
    "secrets-sync": "./dist/secrets-sync.js"
  },
  "scripts": {
    "build": "bun build src/secrets-sync.ts --outdir dist --target node && chmod +x dist/secrets-sync.js",
    "dev": "bun run src/secrets-sync.ts",
    "test": "bun run build && bun test",
    "test:watch": "bun test --watch",
    "lint": "echo 'Linting not configured yet'",
    "quality:duplication": "jscpd src/",
    "quality": "bun run quality:duplication",
    "prepublishOnly": "bun run build && node -e \"if (!process.env.CI) { console.error('âŒ Use CI/CD to publish. Push a version tag instead.'); process.exit(1); }\"",
    "release": "./scripts/release.sh",
    "version:build": "./scripts/bump-build.sh",
    "version:release": "./scripts/bump-release.sh"
  },
  "keywords": [
    "secrets",
    "environment",
    "env",
    "dotenv",
    "sync",
    "github-secrets",
    "cli"
  ],
  "author": "Dorsey Creative",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dorsey-Creative/secrets-sync-cli.git"
  },
  "bugs": {
    "url": "https://github.com/Dorsey-Creative/secrets-sync-cli/issues"
  },
  "homepage": "https://github.com/Dorsey-Creative/secrets-sync-cli#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "latest",
    "jscpd": "^4.0.5"
  },
  "dependencies": {
    "lru-cache": "^11.2.2",
    "yaml": "^2.8.1"
  }
}
